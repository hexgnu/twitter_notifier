<h1>Listing term_counts</h1>

<table>
  <tr>
    <th>Term</th>
    <th>Date</th>
    <th>Tweet</th>
  </tr>

<% @term_counts.each do |term_count| %>
  <tr>
    <td><%=h term_count.term.term %></td>
    <td><%=h term_count.date %></td>
    <td><%=h term_count.tweet_id %></td>
    <td><%= link_to 'Show', term_count %></td>
    <td><%= link_to 'Edit', edit_term_count_path(term_count) %></td>
    <td><%= link_to 'Destroy', term_count, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />